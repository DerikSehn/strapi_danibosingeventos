{
  "kind": "collectionType",
  "collectionName": "budgets",
  "info": {
    "singularName": "budget",
    "pluralName": "budgets",
    "displayName": "Budget",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "eventDetails": {
      "type": "text"
    },
    "eventDuration": {
      "type": "integer"
    },
    "extraHours": {
      "type": "integer"
    },
    "numberOfPeople": {
      "type": "integer"
    },
    "party_type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::party-type.party-type",
      "inversedBy": "budgets"
    },
    "product_variants": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-variant.product-variant",
      "inversedBy": "budgets"
    },
    "totalPrice": {
      "type": "decimal"
    },
    "numberOfWaiters": {
      "type": "integer"
    },
    "eventDate": {
      "type": "datetime",
      "required": false,
      "unique": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pendente",
        "confirmado",
        "em_producao",
        "pronto",
        "entregue",
        "cancelado"
      ],
      "default": "pendente"
    },
    "customerName": {
      "type": "string",
      "required": false
    },
    "customerEmail": {
      "type": "email",
      "required": false
    },
    "customerPhone": {
      "type": "string",
      "required": false
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "budget"
    },
    "total_cost_price": {
      "type": "decimal"
    },
    "margin_percent": {
      "type": "decimal",
      "default": 50
    },
    "internalNotes": {
      "type": "text",
      "required": false
    },
    "deliveryAddress": {
      "type": "text",
      "required": false
    },
    "deliveryCity": {
      "type": "string",
      "required": false
    },
    "deliveryZip": {
      "type": "string",
      "required": false
    },
    "source_channel": {
      "type": "enumeration",
      "enum": ["site", "whatsapp", "presencial", "outro"],
      "required": false
    }
  }
}
